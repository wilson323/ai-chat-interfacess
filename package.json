{
  "name": "ai-chat-interfaces",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 node scripts/timeout-wrapper.js 600 \"next build\"",
    "build:fast": "cross-env NODE_OPTIONS=--max-old-space-size=2048 node scripts/timeout-wrapper.js 600 \"next build --no-lint\"",
    "build:minimal": "cross-env NODE_OPTIONS=--max-old-space-size=1024 node scripts/timeout-wrapper.js 600 \"next build --no-lint --no-typescript\"",
    "start": "next start",
    "lint": "node scripts/timeout-wrapper.js 600 \"next lint\"",
    "lint:fix": "node scripts/timeout-wrapper.js 600 \"next lint --fix\"",
    "format": "node scripts/timeout-wrapper.js 600 \"prettier --write .\"",
    "format:check": "node scripts/timeout-wrapper.js 600 \"prettier --check .\"",
    "check-types": "node scripts/timeout-wrapper.js 600 \"tsc --noEmit\"",
    "type-check": "node scripts/timeout-wrapper.js 600 \"tsc --noEmit --skipLibCheck\"",
    "type-check:strict": "node scripts/timeout-wrapper.js 600 \"tsc --noEmit --strict --skipLibCheck\"",
    "type-check:strict-mode": "node scripts/timeout-wrapper.js 600 \"tsc --noEmit -p ./tsconfig.strict.json\"",
    "type-check:focused": "node scripts/timeout-wrapper.js 600 \"npm run fix-imports && npm run type-check:strict-mode\"",
    "fix-imports": "node scripts/timeout-wrapper.js 600 \"node scripts/automated-type-fix.js\"",
    "fix-paths": "node scripts/timeout-wrapper.js 600 \"node scripts/fix-import-paths.js\"",
    "type-coverage": "node scripts/timeout-wrapper.js 600 \"node scripts/type-safety-check.js\"",
    "type-tests": "node scripts/timeout-wrapper.js 600 \"tsd scripts/type-tests.ts\"",
    "type-safety": "node scripts/timeout-wrapper.js 600 \"npm run type-check && npm run type-coverage && npm run type-tests\"",
    "docs:types": "node scripts/timeout-wrapper.js 600 \"node scripts/generate-type-docs.js\"",
    "docs:types:watch": "node scripts/timeout-wrapper.js 600 \"nodemon --watch lib/ --watch types/ --exec \\\"npm run docs:types\\\"\"",
    "type-dashboard": "node scripts/timeout-wrapper.js 600 \"node scripts/type-safety-dashboard.js\"",
    "verify-types": "node scripts/timeout-wrapper.js 600 \"node scripts/final-type-verification.js\"",
    "test": "node scripts/timeout-wrapper.js 1200 \"jest\"",
    "test:watch": "node scripts/timeout-wrapper.js 600 \"jest --watch\"",
    "validate:resources": "node scripts/validate-static-resources.js",
    "validate:all": "npm run validate:resources && npm run type-check",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "install:browsers": "playwright install",
    "build:timeout": "node scripts/quick-timeout.js \"npm run build\"",
    "type-check:timeout": "node scripts/quick-timeout.js \"npm run type-check:strict\"",
    "test:timeout": "node scripts/quick-timeout.js \"npm run test\"",
    "lint:timeout": "node scripts/quick-timeout.js \"npm run lint\"",
    "quality-check": "node scripts/quality-check.js",
    "quality-deep": "node scripts/code-quality-check.js",
    "quality-full": "npm run quality-check && npm run quality-deep",
    "safe-fix": "node scripts/safe-auto-fix.js",
    "validate-rules": "node scripts/validate-cursor-rules.js",
    "pre-commit": "npm run check-types && npm run lint && npm run test",
    "deploy:vercel": "vercel --prod",
    "deploy:docker": "docker build -t ai-chat-interfaces . && docker run -p 3000:3000 ai-chat-interfaces",
    "deploy:static": "next build && next export",
    "health:check": "curl -f http://localhost:3000/api/health || exit 1",
    "postinstall": "husky install",
    "test:simple": "jest --forceExit",
    "test:single": "jest --testPathPattern=tests/api/unified-agent-manager.test.ts --forceExit",
    "test:debug": "jest --testPathPattern=tests/api/unified-agent-manager.test.ts --verbose --forceExit",
    "test:wsl2": "node scripts/jest-wsl2.js",
    "test:wsl2:single": "node scripts/jest-wsl2.js --testPathPattern=tests/api/unified-agent-manager.test.ts",
    "test:wsl2:debug": "node scripts/jest-wsl2.js --testPathPattern=tests/api/unified-agent-manager.test.ts --verbose",
    "test:alternative": "node scripts/test-alternative.js",
    "test:enhanced": "node scripts/enhanced-test.js",
    "test:verify": "npm run type-check:strict && npm run build",
    "verify:all": "node scripts/final-verification.js",
    "build:wsl2": "node scripts/build-wsl2.js",
    "build:alternative": "node scripts/build-alternative.js",
    "build:smart": "node scripts/smart-build.js",
    "clean": "node scripts/clean-rebuild.js",
    "rebuild": "node scripts/clean-rebuild.js"
  },
  "dependencies": {
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.7.1",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.0.3",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-query": "^5.87.4",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react-window": "^1.8.8",
    "antd": "^5.12.8",
    "axios": "^1.12.1",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.294.0",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "next-themes": "^0.4.6",
    "pg": "^8.11.3",
    "prisma": "^5.7.1",
    "react": "^18",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.3",
    "react-markdown": "^10.1.0",
    "react-window": "^2.1.0",
    "recharts": "^3.2.0",
    "redis": "^5.8.2",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sequelize": "^6.35.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "vitest": "^3.2.4",
    "zod": "^3.22.4",
    "zustand": "^4.4.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "lint-staged": "^15.0.0",
    "postcss": "^8",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "*.{json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
