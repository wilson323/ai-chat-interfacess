{
  "name": "my-v0-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:fix": "eslint . --fix --ext .js,.jsx,.ts,.tsx",
    "check-code": "npm run lint && npm run check-types && npm run format:check",
    "fix-code": "npm run lint:fix && npm run format",
    "check-config": "tsx scripts/check-env-config.ts",
    "check-db": "tsx scripts/check-db.ts",
    "check-custom-ratio": "tsx scripts/check-custom-code-ratio.ts",
    "check-rules": "tsx scripts/check-rules.ts",
    "db:check": "npm run check-db",
    "config:check": "npm run check-config",
    "custom-ratio:check": "npm run check-custom-ratio",
    "rules:check": "npm run check-rules",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__/components",
    "test:integration": "jest --testPathPattern=__tests__/api",
    "test:e2e": "jest --testPathPattern=__tests__/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:optimize": "tsx scripts/optimize-database.ts --analyze --optimize --index",
    "db:backup": "tsx scripts/optimize-database.ts --backup",
    "db:migrate": "tsx scripts/optimize-database.ts --migrate",
    "db:monitor": "tsx scripts/optimize-database.ts --monitor --start-monitoring",
    "db:analyze": "tsx scripts/optimize-database.ts --analyze --verbose",
    "db:index": "tsx scripts/optimize-database.ts --index --create-indexes --drop-unused-indexes",
    "type:check": "tsx scripts/check-type-safety.ts",
    "type:scan": "tsx scripts/check-type-safety.ts && npx tsc --noEmit",
    "type:coverage": "tsx scripts/check-type-safety.ts --coverage",
    "script:safety": "tsx scripts/check-script-safety.ts",
    "script:check": "tsx scripts/check-script-safety.ts",
    "security:scripts": "tsx scripts/check-script-safety.ts",
    "setup:env": "tsx scripts/check-environment.ts",
    "setup:db": "tsx scripts/setup-database.ts",
    "setup:files": "tsx scripts/setup-file-system.ts",
    "setup:test": "tsx scripts/setup-test-environment.ts",
    "setup:production": "tsx scripts/setup-production.ts",
    "setup:all": "tsx scripts/setup-all-environments.ts",
    "setup:dev": "tsx scripts/setup-all-environments.ts -- --env development",
    "setup:test": "tsx scripts/setup-all-environments.ts -- --env test",
    "setup:prod": "tsx scripts/setup-all-environments.ts -- --env production",
    "deploy:check": "npm run setup:env -- --no-redis && npm run setup:production",
    "deploy:production": "npm run setup:production && ./deploy.sh"
  },
